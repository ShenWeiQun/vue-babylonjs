{
  "name": "kaland-vue-babylonjs",
  "version": "0.0.1",
  "files": [
    "dist/*",
    "lib/*"
  ],
  "main": "dist/esm.js",
  "module": "dist/esm.js",
  "unpkg": "dist/umd.min.js",
  "jsdelivr": "dist/umd.min.js",
  "engines": {
    "node": ">=8.9"
  },
  "description": "A ready-to-go 3d environment for Vue.js using Babylon.js",
  "homepage": "https://github.com/Beg-in/vue-babylonjs",
  "repository": "Beg-in/vue-babylonjs",
  "bugs": {
    "url": "https://github.com/Beg-in/vue-babylonjs/issues"
  },
  "author": {
    "name": "Beg.in",
    "email": "info@beg.in",
    "url": "http://beg.in"
  },
  "contributors": [
    "Brian Jesse <brian.jesse@gmail.com> (brianjesse.com)",
    "Johnmark Beaty <johnmarkbeaty@gmail.com> (johnmarkbeaty.com)"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "webpack serve --config ./build/webpack.docs.config.js",
    "start": "webpack-dev-server --config webpack.config.js --progress",
    "build": "webpack --config webpack.config.js",
    "dist:rollup": "rollup --config",
    "dist:uglify": "uglifyjs dist/umd.js -c -m -o dist/umd.min.js",
    "dist": "npm run dist:rollup && npm run dist:uglify",
    "publish": "npm run dist && npm publish --registry=https://maven.klcgis.com/repository/local-npm/"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/plugin-external-helpers": "^7.17.12",
    "@babel/plugin-transform-runtime": "^7.17.12",
    "@babel/preset-env": "^7.17.12",
    "@babel/register": "^7.17.7",
    "@babel/runtime": "^7.17.9",
    "@vue/test-utils": "^2.0.0-rc.21",
    "ava": "^4.2.0",
    "babylonjs": "^5.6.1",
    "babylonjs-gui": "^5.6.1",
    "babylonjs-loaders": "^5.6.1",
    "begin-project": "^0.4.3",
    "cannon": "^0.6.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "earcut": "^2.2.3",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-vue": "^9.0.1",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "find": "^0.3.0",
    "frow": "^4.0.0",
    "glslify-loader": "^2.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jstransformer-markdown-it": "^2.1.0",
    "jstransformer-pug": "^0.4.0",
    "markdown-it-prism": "^2.2.4",
    "material-design-icons": "^3.0.1",
    "node-sass": "^7.0.1",
    "oimo": "^1.0.9",
    "prismjs": "^1.28.0",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "raw-loader": "^4.0.2",
    "rollup": "^2.73.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "sass": "1.38.2",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "stylus": "^0.58.1",
    "stylus-loader": "^7.0.0",
    "uglify-js": "^3.15.5",
    "vue-loader": "^15.9.8",
    "vue-markdown-loader": "^2.5.0",
    "vue-material": "0.8.1",
    "vue-router": "^3.5.3",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "dependencies": {
    "vue": "^2.6.14"
  },
  "bundleDependencies": [
    "babylonjs",
    "babylonjs-gui",
    "babylonjs-loaders"
  ]
}
