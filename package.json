    "@babylonjs/inspector": "^4.0.3",
    "@babylonjs/loaders": "^4.0.3",
    "@vue/test-utils": "^1.0.0-beta.25",
    "ava": "^2.0.0",
    "begin-project": "^0.4.3",
    "cannon": "^0.6.2",
    "css-loader": "^6.6.0",
    "earcut": "^2.1.3",
